
##SDSS query:
select ra,dec,
psfMag_u - extinction_u + -0.04 as psfMagCorr_u,
psfMag_g - extinction_g + 0.00 as psfMagCorr_g,
psfMag_r - extinction_r + 0.00 as psfMagCorr_r,
psfMag_i - extinction_i + 0.00 as psfMagCorr_i,
psfMag_z - extinction_z + 0.02 as psfMagCorr_z,
psfMagErr_u, psfMagErr_g, psfMagErr_r, psfMagErr_i, psfMagErr_z,
-2.5*LOG10(psfFlux_u) + 22.5 - extinction_u + -0.04 as psfPogCorr_u,
-2.5*LOG10(psfFlux_g) + 22.5 - extinction_g + 0.00 as psfPogCorr_g,
-2.5*LOG10(psfFlux_r) + 22.5 - extinction_r + 0.00 as psfPogCorr_r,
-2.5*LOG10(psfFlux_i) + 22.5 - extinction_i + 0.00 as psfPogCorr_i,
-2.5*LOG10(psfFlux_z) + 22.5 - extinction_z + 0.02 as psfPogCorr_z,
1.085/SQRT(psfFluxIvar_u)/psfFlux_u as psfPogErr_u,
1.085/SQRT(psfFluxIvar_g)/psfFlux_g as psfPogErr_g,
1.085/SQRT(psfFluxIvar_r)/psfFlux_r as psfPogErr_r,
1.085/SQRT(psfFluxIvar_i)/psfFlux_i as psfPogErr_i,
1.085/SQRT(psfFluxIvar_z)/psfFlux_z as psfPogErr_z 
from star as s 
JOIN dbo.fGetNearbyObjEq(215.91,24.09,14.67) AS GN ON s.objID = GN.objID 
where s.clean=1 and psfFlux_u > 0 and psfFlux_g > 0 and psfFlux_r > 0 and psfFlux_i > 0 and psfFlux_z > 0 


## Pan-STARRS request form
## http://archive.stsci.edu/panstarrs/search.php
objName 		IAU name for this object
objID 			Unique object identifier
raMean 			Right ascension from single epoch detections (weighted mean  
decMean 		Declination from single epoch detections (weighted mean 
raMeanErr 		Right ascension standard deviation from single epoch detections
decMeanErr 		Declination standard deviation from single epoch detections
nDetections 		Number of single epoch detections in all filters
randomID 		Random value drawn from the interval between zero and one
projectionID 		Projection cell identifier
skyCellID 		Skycell region identifier
objInfoFlag 		Information flag bitmask indicating details of the photometry (see ObjectInfoFlags)
qualityFlag 		Subset of objInfoFlag denoting whether this object is real or a likely false positive (see ObjectQualityFlags)
raStack 		Right ascension from stack detections (J2000)
decStack 		Declination from stack detections (J2000)
raStackErr 		Right ascension standard deviation from stack detections
decStackErr 		Declination standard deviation from stack detections
epochMean 		Modified Julian Date of the mean epoch for raMean  
nStackDetections 	Number of stack detections
ng 			Number of single epoch detections in g filter
nr 			Number of single epoch detections in r filter
ni 			Number of single epoch detections in i filter
nz 			Number of single epoch detections in z filter
